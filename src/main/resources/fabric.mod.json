{
  "schemaVersion": 1,
  "id": "compactmachines-fabric",
  "version": "${version}",
  "name": "Compact Machines Fabric",
  "description": "",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/compactmachines/icon.png",
  "accessWidener": "compactmachines.accesswidener",
  "environment": "*",
  "entrypoints": {
    "client": [
      { 
        "adapter": "scala",
        "value": "us.dison.compactmachines.client.CompactMachinesClient"
      }
    ],
    "main": [ 
      {
        "adapter": "scala",
        "value": "us.dison.compactmachines.CompactMachines"
      }
    ],
    "compactmachines:plugin": [
      {
        "adapter": "scala",
        "value": "us.dison.compactmachines.CompactMachinesCraftingPlugin"
      }
    ]
  },
  "mixins": [
    "compactmachines.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.2",
    "fabric-language-scala": "*"
  },
  "custom": {
    "waila:plugins": {
      "id": "compactmachines:plugin", 
      "initializer": "us.dison.compactmachines.compat.CompactWthitPlugin"
    }
  }
}
